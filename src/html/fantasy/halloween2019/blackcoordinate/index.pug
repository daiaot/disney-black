extend _template

block vars
  - title = '東京ディズニーリゾート'
  - description = ''
  - keywords = ''
  - img = ''
  - url = ''

block main
  //- div(data-module='dummy' data-options='{ "dummy_opt": "dummy" }') Click me!
  //- div(data-module='legacy') Legacy!

  | #{"\n"}
  .wrap
    //{* ヘッダーインクルード *}
    //#include virtual="/ssi/header_event_ps_inc.html"
    //- include /ssi/header_event_ps_inc.pug

    //{* /ヘッダーインクルード *}
    | #{"\n"}
    .contents-inner.breadcrumb_area.pc
      //{* ここにパンくずを入れてください PC *}
      .breadcrumb
        ul.inner
          li
            span.text-buttons.text-buttons-s
              a(href='/top.html') TOP
          li.current
            span ブラックコーデディズニー
      //{* /ここにパンくずを入れてください PC *}

    | #{"\n"}
    //{* コンテンツエリア *}
    article#content-area

      .sec-inner
        // KV
        .sec-block
          .youtube-wrap
            .youtube-content
              #player
            .
              <div class="youtube-content__scroll"> <span>SCROLL</span> </div>

        .sec-block
          .sec-block__lead

          .slide-wrap(data-module='slider')
            .slide-wrap__image
              img(src=`images/img_coordinate01.jpg` alt="")
            //- .slide-wrap__image
            //-   img(src=`images/img_coordinate02.jpg` alt="")
            .slide-wrap__image
              img(src=`images/img_coordinate03.jpg` alt="")


        .sec-block
          h2.sec-block__title
           | 東京ディズニーリゾート公式 Instagramにあなたが登場!?

          p.sec-block__text
            | オフィシャルフォトグラファーに撮影された写真は
            br
            | 東京ディズニーリゾート公式サイトと
            br
            | インスタグラムストーリーズにアップされるかも!?

          .insta-wrap
            .insta-wrap__kv
            a.sec-btn__dl(href="http://www.tokyodisneyresort.jp/photo/index02.html" target='_blank')
            .insta-wrap__stories
              ul
                li
                  img(src=`images/img_stories01.png` alt="")
                li
                  img(src=`images/img_stories02.png` alt="")
                li
                  img(src=`images/img_stories03.png` alt="")
            a.sec-btn__instagram(href="https://www.instagram.com/tokyodisneyresort_official/" target='_blank')



        .sec-block
          h2.sec-block__title
            | みんなのブラックコーデを見てみよう！

          //- .gallery-wrap(data-module='showMore')
          .gallery-wrap
            h3.sec-block__lead
              | American Waterfront
              | アメリカンウォーターフロントで撮影した写真を見てみよう!
            ul.gallery-wrap__lists._showmore
              li
                img(data-module="modal" data-large="images/img_thumb_01_l.jpg" src=`images/img_thumb_01.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_02_l.jpg" src=`images/img_thumb_02.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_03_l.jpg" src=`images/img_thumb_03.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_04_l.jpg" src=`images/img_thumb_04.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_05_l.jpg" src=`images/img_thumb_05.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_06_l.jpg" src=`images/img_thumb_06.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_07_l.jpg" src=`images/img_thumb_07.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_08_l.jpg" src=`images/img_thumb_08.jpg` alt="")
            //- .sec-btn__showmore

          .gallery-wrap
            h3.sec-block__lead
              |Arabian Coast
              |アラビアンコーストで撮影した写真を見てみよう!
            ul.gallery-wrap__lists._showmore
              li
                img(data-module="modal" data-large="images/img_thumb_09_l.jpg" src=`images/img_thumb_09.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_10_l.jpg" src=`images/img_thumb_10.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_11_l.jpg" src=`images/img_thumb_11.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_12_l.jpg" src=`images/img_thumb_12.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_13_l.jpg" src=`images/img_thumb_13.jpg` alt="")
              li
                img(data-module="modal" data-large="images/img_thumb_14_l.jpg" src=`images/img_thumb_14.jpg` alt="")

          .gallery-wrap
            h3.sec-block__lead
              |Mermaid Lagoon
              |マーメイドラグーンで撮影した写真を見てみよう!
            ul.gallery-wrap__lists._showmore
              li
                img(data-module="modal" data-large="images/img_thumb_15_l.jpg" src=`images/img_thumb_15.jpg` alt="")

          .gallery-wrap
            h3.sec-block__lead
              |Mediterranean Harbor
              |メディテレーニアンハーバーで撮影した写真を見てみよう!
            ul.gallery-wrap__lists._showmore
              li
